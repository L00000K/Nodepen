{
  "name": "glasshopper.io",
  "version": "1.0.0",
  "description": "the grasshopper glossary",
  "author": "chuck",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development ts-node --project 'server/tsconfig.json' server/index.ts --watch server",
    "build": "tsc --p server/tsconfig.json && nuxt build",
    "build:express": "tsc --p server/tsconfig.json",
    "start:ts": "cross-env NODE_ENV=production ts-node --project 'server/tsconfig.json' server/index.ts",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "test": "jest"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.8",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@types/jest": "^25.2.3",
    "@types/socket.io": "^2.1.8",
    "@types/socket.io-client": "^1.4.33",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "nuxt": "^2.0.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "svgar-server": "0.0.3",
    "ts-node": "^8.10.2"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/tailwindcss": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.1.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "vue-jest": "^4.0.0-0"
  }
}
