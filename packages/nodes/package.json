{
  "name": "@nodepen/nodes",
  "version": "1.1.0",
  "description": "node editor component library for nodepen",
  "author": "@cdriesler",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodepen/nodes.git"
  },
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "prebuild": "tailwindcss build -i tailwind.css -o dist/styles.css",
    "build": "tsc",
    "postbuild": "tsc-alias",
    "dev": "concurrently \"tsc-watch --noClear --onSuccess \\\"npm run prebuild\\\"\" \"tsc-alias -w\"",
    "link:react": "npm link ../../node_modules/react"
  },
  "dependencies": {
    "@nodepen/core": "^1.1.0",
    "immer": "9.0.15",
    "zustand": "4.1.1"
  },
  "peerDependencies": {
    "react": "^18.0",
    "react-dom": "^18.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.6",
    "tailwindcss": "3.1.8"
  }
}
