{
  "name": "@nodepen/nodes",
  "version": "1.1.0",
  "description": "node editor component library for nodepen",
  "author": "@cdriesler",
  "repository": {
    "type": "git",
    "url": "https://github.com/nodepen/nodes.git"
  },
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "prebuild": "tailwindcss build -i tailwind.css -o dist/styles.css && copyfiles assets/*.ttf dist",
    "build": "tsc",
    "postbuild": "tsc-alias",
    "dev": "concurrently \"tsc-watch --noClear --onSuccess \\\"npm run prebuild\\\"\" \"tsc-alias -w\"",
    "test": "jest"
  },
  "dependencies": {
    "@nodepen/core": "^1.1.0",
    "@speckle/viewer": "^2.11.4",
    "immer": "^9.0.18",
    "zustand": "^4.3.2"
  },
  "peerDependencies": {
    "react": "^18.0",
    "react-dom": "^18.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/three": "^0.148.1",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.21",
    "tailwindcss": "^3.2.4"
  }
}
