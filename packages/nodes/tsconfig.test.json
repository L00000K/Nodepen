{
  "extends": "../../tsconfig.base.json",
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "dist",
    "node_modules",
  ],
  "compilerOptions": {
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "$": ["./src/store"]
    },
    "module": "esnext",
    "target": "esnext",
    "verbatimModuleSyntax": false,
    "types": [
      "mocha",
      "jest"
    ]
  }
}